      "source": [
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "\n",
        "# Extract the hour from the 'Time' column to create 'Hour_Category'\n",
        "df_sensor['Hour_Category'] = df_sensor['Time'].dt.hour\n",
        "\n",
        "print(\"First 5 rows with new 'Hour_Category' column:\")\n",
        "print(df_sensor.head())\n",
        "\n",
        "# Generate a swarm plot to visualize Temperature_C distribution across 'Hour_Category' groups\n",
        "plt.figure(figsize=(14, 7))\n",
        "sns.swarmplot(data=df_sensor, x='Hour_Category', y='Temperature_C')\n",
        "plt.title('Temperature Distribution by Hour of Day')\n",
        "plt.xlabel('Hour of Day')\n",
        "plt.ylabel('Temperature_C')\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
