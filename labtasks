      "source": [
        "print(\"\\nFirst 5 rows of the data loaded with numpy.genfromtxt():\")\n",
        "try:\n",
        "    data_genfromtxt = np.genfromtxt(\n",
        "        'airline_booking_records.csv',\n",
        "        delimiter=',',\n",
        "        skip_header=1,\n",
        "        dtype=None, # Allow for mixed data types\n",
        "        encoding='utf-8'\n",
        "    )\n",
        "    print(\"Successfully loaded data using numpy.genfromtxt().\")\n",
        "    # genfromtxt returns a 1D array of tuples if dtype=None and records are not uniform\n",
        "    # If it's a 2D array, slice directly. If 1D array of tuples, convert to list for printing first few.\n",
        "    if data_genfromtxt.ndim == 1 and data_genfromtxt.dtype.names is not None:\n",
        "        # It's a structured array, print first 5 records\n",
        "        for i, row in enumerate(data_genfromtxt[:5]):\n",
        "            print(row)\n",
        "    elif data_genfromtxt.ndim > 0:\n",
        "        # It's a regular array, print first 5 rows\n",
        "        print(data_genfromtxt[:5])\n",
        "    else:\n",
        "        print(data_genfromtxt)\n",
        "except Exception as e:\n",
        "    print(f\"Error loading with numpy.genfromtxt():\\n{e}\")"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "\n",
            "First 5 rows of the data loaded with numpy.genfromtxt():\n",
