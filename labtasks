      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "d2f6574e",
        "outputId": "b0579aca-2dcf-493d-9786-84f3c4ddcee2"
      },
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "from datetime import datetime, timedelta\n",
        "\n",
        "source_cities = ['New York', 'London', 'Paris', 'Tokyo', 'Dubai', 'Singapore', 'Sydney', 'Mumbai']\n",
        "destination_cities = ['London', 'Paris', 'Tokyo', 'Dubai', 'New York', 'Singapore', 'Sydney', 'Mumbai']\n",
        "flight_classes = ['Economy', 'Business', 'First Class']\n",
        "\n",
        "num_records = 50\n",
        "\n",
        "booking_ids = [f'BK{i:05d}' for i in range(1, num_records + 1)]\n",
        "\n",
        "sources = np.random.choice(source_cities, num_records)\n",
        "destinations = np.random.choice(destination_cities, num_records)\n",
        "\n",
        "for i in range(num_records):\n",
        "    while sources[i] == destinations[i]:\n",
        "        destinations[i] = np.random.choice(destination_cities)\n",
        "\n",
        "flight_ids = [''.join(np.random.choice(list('ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789'), 6)) for _ in range(num_records)]\n",
        "\n",
        "classes = np.random.choice(flight_classes, num_records)\n",
        "amounts = np.random.randint(10000, 60001, num_records)\n",
        "current_date = datetime.now()\n",
        "booking_dates = [current_date - timedelta(days=np.random.randint(1, 31)) for _ in range(num_records)] # Last 30 days\n",
        "travelled_dates = []\n",
        "\n",
        "for i in range(num_records):\n",
        "    travel_delay_days = np.random.randint(1, 91)\n",
        "    travelled_dates.append(booking_dates[i] + timedelta(days=travel_delay_days))\n",
        "\n",
        "data = {\n",
        "    'Booking_ID': booking_ids,\n",
        "    'Source': sources,\n",
        "    'Destination': destinations,\n",
        "    'Flight_ID': flight_ids,\n",
        "    'Class': classes,\n",
        "    'Amount': amounts,\n",
        "    'Booking_Date': booking_dates,\n",
        "    'Travelled_Date': travelled_dates\n",
        "}\n",
        "df_bookings = pd.DataFrame(data)\n",
        "\n",
        "df_bookings['Booking_Date'] = df_bookings['Booking_Date'].dt.strftime('%Y-%m-%d')\n",
        "df_bookings['Travelled_Date'] = df_bookings['Travelled_Date'].dt.strftime('%Y-%m-%d')\n",
        "\n",
        "csv_file_path = 'airline_booking_records.csv'\n",
        "df_bookings.to_csv(csv_file_path, index=False)\n",
        "\n",
        "print(f\"Successfully created '{csv_file_path}' with {num_records} booking records.\")\n",
        "print(\"First 5 rows of the generated DataFrame:\")\n",
        "print(df_bookings.head())"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Successfully created 'airline_booking_records.csv' with 50 booking records.\n",
            "First 5 rows of the generated DataFrame:\n",
            "  Booking_ID     Source Destination Flight_ID        Class  Amount  \\\n",
            "0    BK00001     Mumbai      London    93FWIP      Economy   16419   \n",
            "1    BK00002     London       Dubai    CAFW4W  First Class   24536   \n",
